{
    "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
    "name": "io.github.ankitpal181/toon-parse-mcp",
    "description": "MCP server that reduces LLM context by removing code comments and converting data formats to TOON",
    "repository": {
        "url": "https://github.com/ankitpal181/toon-parse-mcp",
        "source": "github"
    },
    "version": "1.0.0-beta",
    "packages": [
        {
            "registryType": "pypi",
            "identifier": "toon-parse-mcp",
            "version": "1.0.1-beta",
            "transport": {
                "type": "stdio"
            },
            "runtime": {
                "command": "python",
                "args": [
                    "-m",
                    "src.server"
                ]
            }
        }
    ]
}